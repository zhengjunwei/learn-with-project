<!DOCTYPE html>
<html>
<head>
  <title>LearnWithProject</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
	<!-- <header class="navbar navbar-fixed-top navbar-static-top bs-docs-nav"> -->

	<header class="navbar navbar-inverse navbar-fixed-top custom-header">
		<div class="container container-left">
			<div class="navbar-header navbar-header-wrapper">
				<button class="navbar-toggle collapsed"></button>
				<%= image_tag("try-ruby.png", class: "nav-img-box", alt: "try-ruby") %>
			</div>
			<nav class="collapse navbar-collapse bs-navbar-collapse">
				<ul class="nav navbar-nav navbar-nav-white">
				</ul>
				<ul class="nav navbar-nav navbar-right navbar-nav-white nav-wrapper">
					<li><a href="#">Home</a></li>
					<li><a href="#">About</a></li>
					<li><a href="#">Contact</a></li>
					<li><a href="#">Signup</a></li>
				</ul>
			</nav>
		</div>
	</header>
	<div class="jumbotron jumbotron-white-bg">
		<%= yield %>
	</div>
	<div class="container-fluid">
		<%= render partial: "shared/content" %>
	</div>
	<div class="container-fluid">
		<hr>
		<%= render partial: 'shared/footer' %>	
	</div>
</body>
</html>
